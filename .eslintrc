{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": { "es6": true },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true,
          "modules": true,
          "experimentalObjectRestSpread": true
        }
    },
    "globals": {
        "document": false,
        "require": false,
        "module": false,
        "__dirname": false,
        "window": false,
        "setTimeout": false
    }
}